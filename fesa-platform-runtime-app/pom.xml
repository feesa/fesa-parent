		<springversion>4.0.9.RELEASE</springversion>
		<springsecurityversion>3.2.5.RELEASE</springsecurityversion>
		<hibernateversion>4.2.16.Final</hibernateversion>
	</properties>
	<dependencies>
		<dependency>
			<groupId>junit</groupId>
